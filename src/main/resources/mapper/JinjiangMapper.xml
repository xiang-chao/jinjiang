<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--mapper为根元素，namespace指定了命名空间-->
<mapper namespace="com.jinjiang.mapper.JinjiangMapper">

   <insert id="saveJinJiang">
       insert into jinjiang(orderCode, cardNo,create_time) values (#{orderCode},#{cardNo},now())
   </insert>

</mapper>